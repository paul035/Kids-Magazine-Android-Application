<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.magazines">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegActivity" />
        <activity android:name=".BookActivity" />
        <activity android:name=".DrawingBookActivity" />
        <activity android:name=".MusicActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ShortSotriesActivity" />
        <activity android:name=".DescriptionActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".AccountBookActivity"></activity>
    </application>

</manifest>